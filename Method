using System;
using System.Collections.Generic;
using System.Linq;
using System.Text;
using System.Threading.Tasks;

namespace PR0403_KDH23
{
    class Program
    {
        static void Main(string[] args)
        {
            double c, d, area;        //변수 선언
            c = 10; d = 20;           //변수 초기화
            area = abc(c, d);         //c,d 를 매개변수로 갖는 메소드 호출(정적 메소드 이기 때문에 객체 생성 하지 않아도 된다)
            Console.WriteLine(area);  //area 출력
        }

        static double abc(double a, double b) //abc 라는 이름의 정적 메소드 선언
        {
            double ab;                        //ab 변수 선언
            ab = a * b;                       //ab 변수에 a*b 연산값 배정
            return (ab);                      /*메소드를 호출한 곳에 ab 값을 되돌려준다
                                              메소드를 호출한 area 변수에 ab 값이 배정된다*/
        
    }
}
}
